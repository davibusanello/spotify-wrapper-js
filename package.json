{
  "name": "spotify-wrapper-js",
  "version": "0.0.1",
  "description": "A wrapper to work with the Spotify Web API.",
  "main": "main.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require @babel/register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require @babel/register --watch --reporter=nyan",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davibusanello/spotify-wrapper-js.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library",
    "study"
  ],
  "author": "Davi Busanello <itsme@davibusanello.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davibusanello/spotify-wrapper-js/issues"
  },
  "homepage": "https://github.com/davibusanello/spotify-wrapper-js#readme",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.1.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "node-fetch": "^2.2.0"
  }
}
